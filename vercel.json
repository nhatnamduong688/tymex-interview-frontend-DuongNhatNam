{
  "framework": "vite",
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "trailingSlash": false,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "continue": true
    }
  ]
}
